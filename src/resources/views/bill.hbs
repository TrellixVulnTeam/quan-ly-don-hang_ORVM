<div class="row mt-3">
    {{#each products}}
    <div class="col-sm-4 col-md-3 col-lg-3 col-xl-3">
        <div class="card mb-3">
            <div class="card-body">
                <h6 class="card-title">Mã: <strong>{{this.productId}}</strong></h6>
                <h6 class="card-title"><strong>{{this.productName}}</strong></h6>
                <p class="card-text">Số lượng: {{this.productAmount}}</p>
                <p class="card-text">Đơn giá: {{this.productPrice}} VNĐ</p>
                <p class="card-text">Ngày nhập: {{this.productDateIn}}</p>
                <p class="card-text">Ngày xuất: {{this.productDateOut}}</p>
                <a href="#" class="card-link">Sửa</a>
                <a href="#" class="card-link">Xóa</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>